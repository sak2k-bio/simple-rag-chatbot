{
  "name": "bulk-pdf-processor",
  "version": "1.0.0",
  "description": "Production-ready bulk PDF processor with optimized chunking and paragraph awareness",
  "main": "bulk-pdf-processor-optimized.js",
  "scripts": {
    "start": "node web-ui/server.js",
    "process": "node bulk-pdf-processor-optimized.js",
    "docker:build": "docker build -t bulk-pdf-processor .",
    "docker:run": "docker run -p 3001:3001 -v $(pwd)/data:/app/data -v $(pwd)/uploads:/app/uploads -v $(pwd)/manifests:/app/manifests bulk-pdf-processor",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "fast-glob": "^3.3.2",
    "p-limit": "^4.0.0",
    "p-retry": "^6.2.0",
    "better-sqlite3": "^9.2.2",
    "@qdrant/js-client-rest": "^1.7.0",
    "@ai-sdk/google": "^0.0.66",
    "ai": "^3.0.0",
    "pdf-parse": "^1.1.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "pdf",
    "embeddings",
    "vector-database",
    "qdrant",
    "rag",
    "chunking",
    "medical",
    "bulk-processing"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
